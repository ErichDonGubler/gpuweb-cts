{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [false],
    "max-classes-per-file": false,
    "max-line-length": [true, 100],
    "no-bitwise": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  }
}
